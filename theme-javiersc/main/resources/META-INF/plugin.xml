<idea-plugin>
    <id>com.javiersc.intellij.theme</id>
    <name>IDEA Theme - JavierSC</name>
    <vendor>JavierSC</vendor>
    <description>JavierSC light and dark themes for IntelliJ IDEA</description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="9c995c89-8c6f-44fc-b41b-73d43dab952d" path="javiersc_dark.theme.json"/>
        <themeProvider id="com.javiersc.intellij.theme.javiersc.new.dark" path="javiersc_new_dark.theme.json"/>
    </extensions>
</idea-plugin>
